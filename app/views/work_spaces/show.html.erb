<section class="notification is-primary  is-bold level">
  <div class="level-left ">
    <h1 class="title "><%= @work_space.name %></h1>
  </div>
  <div class="level-right">
    <%= render 'shared/invite_btn' %>
  </div>
</section>
<section class="section">
  <div class="columns">

    <div class="column  is-narrow">
      <%= render partial: 'shared/admin_menu',  locals: { work_space_id: @work_space.id }%>
    </div>
    <div class="column content">
      <p>
        Polling stations:
      </p>

      <ul>
        <% @work_space.polling_stations.each do |ps| %>
          <li><%= ps.name %></li>
        <% end %>
      </ul>
    </div>
  </div>


</section>

